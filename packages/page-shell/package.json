{
  "name": "@repo/page-shell",
  "version": "0.1.0",
  "description": "Unified barrel package for PageShell - re-exports from all @pageshell/* packages",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./core": "./src/core.ts",
    "./primitives": "./src/primitives.ts",
    "./layouts": "./src/layouts.ts",
    "./interactions": "./src/interactions.ts",
    "./features": "./src/features.ts",
    "./composites": "./src/composites.ts",
    "./theme": "./src/theme.ts",
    "./themes": "./src/themes.ts",
    "./domain": "./src/domain.ts"
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@pageshell/composites": "workspace:*",
    "@pageshell/core": "workspace:*",
    "@pageshell/domain": "workspace:*",
    "@pageshell/features": "workspace:*",
    "@pageshell/interactions": "workspace:*",
    "@pageshell/layouts": "workspace:*",
    "@pageshell/primitives": "workspace:*",
    "@pageshell/shell": "workspace:*",
    "@pageshell/theme": "workspace:*",
    "@pageshell/themes": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-hook-form": "^7.70.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "private": true
}
